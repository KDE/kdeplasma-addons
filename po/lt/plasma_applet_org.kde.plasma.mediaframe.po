#: qml/config.qml:13
#, kde-format
msgctxt "@title"
msgid "Media"
msgstr "Medija"

#: qml/config.qml:18
#, kde-format
msgctxt "@title"
msgid "Behavior"
msgstr "Elgsena"

#: qml/ConfigBehavior.qml:48
#, kde-format
msgid "Change picture every:"
msgstr "Keisti paveikslą kas:"

#: qml/ConfigBehavior.qml:65
#, kde-format
msgid "Hours"
msgstr "val."

#: qml/ConfigBehavior.qml:79
#, kde-format
msgid "Minutes"
msgstr "min."

#: qml/ConfigBehavior.qml:93
#, kde-format
msgid "Seconds"
msgstr "sek."

#: qml/ConfigBehavior.qml:103
#, kde-format
msgctxt "@label:listbox"
msgid "Image fill mode:"
msgstr "Paveikslo užpildymo veiksena:"

#: qml/ConfigBehavior.qml:110
#, kde-format
msgctxt "@item:inlistbox"
msgid "Stretch"
msgstr "Ištempti"

#: qml/ConfigBehavior.qml:111
#, kde-format
msgctxt "@info"
msgid "The image is scaled to fit the frame"
msgstr "Paveikslo mastelis bus pakeistas taip, kad paveikslas užpildytų rėmelį"

#: qml/ConfigBehavior.qml:115
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect fit"
msgstr "Talpinti, išlaikyti proporcijas"

#: qml/ConfigBehavior.qml:116
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fit without cropping"
msgstr "Paveikslo mastelis bus pakeistas tolygiai taip, kad paveikslas tilptų"

#: qml/ConfigBehavior.qml:120
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect crop"
msgstr "Apkarpyti, išlaikyti proporcijas"

#: qml/ConfigBehavior.qml:121
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fill, cropping if necessary"
msgstr ""
"Paveikslo mastelis bus pakeistas tolygiai ir, jei reikia, apkarpytas taip, "
"kad paveikslas tilptų"

#: qml/ConfigBehavior.qml:125
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile"
msgstr "Iškloti"

#: qml/ConfigBehavior.qml:126
#, kde-format
msgctxt "@info"
msgid "The image is duplicated horizontally and vertically"
msgstr "Paveikslas bus kartojamas horizontaliai ir vertikaliai"

#: qml/ConfigBehavior.qml:130
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile vertically"
msgstr "Iškloti vertikaliai"

#: qml/ConfigBehavior.qml:131
#, kde-format
msgctxt "@info"
msgid "The image is stretched horizontally and tiled vertically"
msgstr "Paveikslas bus ištemptas horizontaliai ir išklotas vertikaliai"

#: qml/ConfigBehavior.qml:135
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile horizontally"
msgstr "Iškloti horizontaliai"

#: qml/ConfigBehavior.qml:136
#, kde-format
msgctxt "@info"
msgid "The image is stretched vertically and tiled horizontally"
msgstr "Paveikslas bus ištemptas vertikaliai ir išklotas horizontaliai"

#: qml/ConfigBehavior.qml:140
#, kde-format
msgctxt "@item:inlistbox"
msgid "Pad"
msgstr "Padėti"

#: qml/ConfigBehavior.qml:141
#, kde-format
msgctxt "@info"
msgid "The image is not transformed"
msgstr "Paveikslas nebus transformuojamas"

#: qml/ConfigBehavior.qml:186
#, kde-format
msgctxt "@label:checkbox"
msgid "General:"
msgstr "Bendra:"

#: qml/ConfigBehavior.qml:187
#, kde-format
msgctxt "@option:check"
msgid "Randomize order"
msgstr "Rodyti atsitiktine tvarka"

#: qml/ConfigBehavior.qml:192
#, kde-format
msgctxt "@option:check"
msgid "Pause slideshow when cursor is over image"
msgstr "Užvedus pelės žymeklį ant paveikslo, pristabdyti skaidrių rodymą"

#: qml/ConfigBehavior.qml:197
#, kde-format
msgctxt "@option:check"
msgid "Click on image to open in external application"
msgstr "Spustelėjus ant paveikslo, atverti jį naudojant išorinę programą"

#: qml/ConfigMedia.qml:27
#, kde-format
msgctxt "@action:button"
msgid "Add Folder…"
msgstr "Pridėti aplanką…"

#: qml/ConfigMedia.qml:32
#, kde-format
msgctxt "@action:button"
msgid "Add Files…"
msgstr "Pridėti failus…"

#: qml/ConfigMedia.qml:84
#, kde-format
msgctxt "@action:button tooltip on icon-only button"
msgid "Remove path"
msgstr "Šalinti kelią"

#: qml/ConfigMedia.qml:94
#, kde-format
msgctxt "@info placeholdermessage if no paths set"
msgid "No media configured"
msgstr "Nesukonfigūruota jokia medija"

#: qml/ConfigMedia.qml:95
#, kde-format
msgctxt "@info placeholdermessage explanation"
msgid "Add files or folders to display them in the widget."
msgstr "Pridėti failų ar aplankų, kad jie būtų rodomi valdiklyje."

#: qml/ConfigMedia.qml:101
#, kde-format
msgctxt "@title:window"
msgid "Choose Files"
msgstr "Pasirinkite failus"

#: qml/ConfigMedia.qml:121
#, kde-format
msgctxt "@title:window"
msgid "Choose a Folder"
msgstr "Pasirinkite aplanką"

#: qml/main.qml:360
#, kde-format
msgctxt "@info placeholdermessage if no media loaded"
msgid "No media selected"
msgstr "Nepasirinkta jokia medija"

#: qml/main.qml:364 qml/main.qml:374
#, kde-format
msgctxt "@action:button opens settings dialog"
msgid "Choose media…"
msgstr "Pasirinkti mediją…"

