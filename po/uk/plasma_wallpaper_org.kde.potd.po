#: package/contents/ui/config.qml:43
#, kde-format
msgctxt "@label:listbox"
msgid "Provider:"
msgstr "Надавач:"

#: package/contents/ui/config.qml:62
#, kde-format
msgctxt "@info:tooltip"
msgid ""
"This wallpaper provider does not filter out images that may be sensitive or "
"objectionable. Use caution if these images will be displayed in public."
msgstr ""
"Цей надавач фонових зображень не фільтрує зображення, які можуть бути "
"образливими або такими, що містять об'єктивацію. Будьте обережні із показом "
"цих зображень на публіці."

#: package/contents/ui/config.qml:71
#, kde-format
msgctxt "@option:check"
msgid "Update when using metered network connection"
msgstr "Оновлювати при використанні з'єднання із обліком даних"

#: package/contents/ui/config.qml:80
#, kde-format
msgctxt "@label:listbox"
msgid "Positioning:"
msgstr "Розташування:"

#: package/contents/ui/config.qml:83
#, kde-format
msgctxt "@item:inlistbox"
msgid "Scaled and Cropped"
msgstr "Масштабоване та обрізане"

#: package/contents/ui/config.qml:87
#, kde-format
msgctxt "@item:inlistbox"
msgid "Scaled"
msgstr "Масштабоване"

#: package/contents/ui/config.qml:91
#, kde-format
msgctxt "@item:inlistbox"
msgid "Scaled, Keep Proportions"
msgstr "Масштабоване, зі збереженням пропорцій"

#: package/contents/ui/config.qml:95
#, kde-format
msgctxt "@item:inlistbox"
msgid "Centered"
msgstr "За центром"

#: package/contents/ui/config.qml:99
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tiled"
msgstr "Плиткою"

#: package/contents/ui/config.qml:120
#, kde-format
msgctxt "@label:chooser"
msgid "Background color:"
msgstr "Колір тла:"

#: package/contents/ui/config.qml:121
#, kde-format
msgctxt "@title:window"
msgid "Select Background Color"
msgstr "Виберіть колір тла"

#: package/contents/ui/config.qml:132
#, kde-format
msgctxt "@label"
msgid "Today's picture:"
msgstr "Сьогоднішнє зображення:"

#: package/contents/ui/config.qml:148
#, kde-format
msgctxt "@label"
msgid "Title:"
msgstr "Заголовок:"

#: package/contents/ui/config.qml:163
#, kde-format
msgctxt "@label"
msgid "Author:"
msgstr "Автор:"

#: package/contents/ui/config.qml:182
#, kde-format
msgctxt "@action:button"
msgid "Open Containing Folder"
msgstr "Відкрити теку з даними"

#: package/contents/ui/config.qml:186
#, kde-format
msgctxt "@info:whatsthis for a button"
msgid "Open the destination folder where the wallpaper image was saved."
msgstr "Відкрити теку призначення, до якої було збережено зображення тла."

#: package/contents/ui/main.qml:22
#, kde-format
msgid "Open Wallpaper Image"
msgstr "Відкрити зображення тла стільниці"

#: package/contents/ui/WallpaperDelegate.qml:137
#, kde-format
msgctxt "@info:whatsthis"
msgid "Today's picture"
msgstr "Сьогоднішнє зображення"

#: package/contents/ui/WallpaperDelegate.qml:138
#, kde-format
msgctxt "@info:whatsthis"
msgid "Loading"
msgstr "Завантаження"

#: package/contents/ui/WallpaperDelegate.qml:139
#, kde-format
msgctxt "@info:whatsthis"
msgid "Unavailable"
msgstr "Немає доступу"

#: package/contents/ui/WallpaperDelegate.qml:140
#, kde-format
msgctxt "@info:whatsthis for an image %1 title %2 author"
msgid "%1 Author: %2. Right-click on the image to see more actions."
msgstr ""
"Автор %1: %2. Клацніть правою кнопкою миші на зображення, щоб отримати "
"доступ до інших дій."

#: package/contents/ui/WallpaperDelegate.qml:141
#, kde-format
msgctxt "@info:whatsthis"
msgid "The wallpaper is being fetched from the Internet."
msgstr "Отримуємо зображення тла з інтернету."

#: package/contents/ui/WallpaperDelegate.qml:142
#, kde-format
msgctxt "@info:whatsthis"
msgid "Failed to fetch the wallpaper from the Internet."
msgstr "Не вдалося отримати зображення тла з інтернету."

#: package/contents/ui/WallpaperPreview.qml:49
#, kde-format
msgctxt "@action:inmenu wallpaper preview menu"
msgid "Save Image as…"
msgstr "Зберегти зображення як…"

#: package/contents/ui/WallpaperPreview.qml:52
#, kde-format
msgctxt "@info:whatsthis for a button and a menu item"
msgid "Save today's picture to local disk"
msgstr "Зберегти сьогоднішнє зображення на локальному диску"

#: package/contents/ui/WallpaperPreview.qml:58
#, kde-format
msgctxt ""
"@action:inmenu wallpaper preview menu, will open the website of the wallpaper"
msgid "Open Link in Browser…"
msgstr "Відкрити посилання в інтернет-навігаторі…"

#: package/contents/ui/WallpaperPreview.qml:61
#, kde-format
msgctxt "@info:whatsthis for a menu item"
msgid "Open the website of today's picture in the default browser"
msgstr ""
"Відкрити сайт сьогоднішнього зображення у типовій програмі для перегляду "
"інтернету"

#: plugins/potdbackend.cpp:213
#, kde-format
msgctxt "@title:window"
msgid "Save Today's Picture"
msgstr "Зберегти сьогоднішнє зображення"

#: plugins/potdbackend.cpp:215
#, kde-format
msgctxt "@label:listbox Template for file dialog"
msgid "JPEG image (*.jpeg *.jpg *.jpe)"
msgstr "зображення JPEG (*.jpeg *.jpg *.jpe)"

#: plugins/potdbackend.cpp:232
#, kde-format
msgctxt "@info:status after a save action"
msgid "The image was not saved."
msgstr "Зображення не збережено."

#: plugins/potdbackend.cpp:239
#, kde-format
msgctxt "@info:status after a save action %1 file path %2 basename"
msgid "The image was saved as <a href=\"%1\">%2</a>"
msgstr "Зображення збережено як <a href=\"%1\">%2</a>"

