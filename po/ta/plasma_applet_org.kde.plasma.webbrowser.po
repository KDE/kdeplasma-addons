#: qml/config.qml:12
#, kde-format
msgctxt "@title"
msgid "General"
msgstr "பொதுவானவை"

#: qml/config.qml:18
#, kde-format
msgctxt "@title"
msgid "Appearance"
msgstr "தோற்றம்"

#: qml/ConfigAppearance.qml:33
#, kde-format
msgctxt "@title:group"
msgid "Icon:"
msgstr "சின்னம்:"

#: qml/ConfigAppearance.qml:34
#, kde-format
msgctxt "@option:radio"
msgid "Website's favicon"
msgstr "இணையதளத்தின் சின்னம்"

#: qml/ConfigAppearance.qml:57
#, kde-format
msgctxt "@action:button"
msgid "Change Web Browser's icon"
msgstr "இணைய உலாவியின் சின்னத்தை மாற்று"

#: qml/ConfigAppearance.qml:58
#, kde-format
msgctxt "@info:whatsthis"
msgid ""
"Current icon is %1. Click to open menu to change the current icon or reset "
"to the default icon."
msgstr ""
"தற்போதைய சின்னம் %1 ஆகும். சின்னத்தை மாற்ற அல்லது இயல்பிருப்பாக மீட்டமைக்க கிளிக் செய்யவும்."

#: qml/ConfigAppearance.qml:62
#, kde-format
msgctxt "@info:tooltip"
msgid "Icon name is \"%1\""
msgstr "சின்னத்தின் பெயர் \"%1\""

#: qml/ConfigAppearance.qml:97
#, kde-format
msgctxt "@item:inmenu Open icon chooser dialog"
msgid "Choose…"
msgstr "தேர்வு செய்…"

#: qml/ConfigAppearance.qml:99
#, kde-format
msgctxt "@info:whatsthis"
msgid "Choose an icon for Web Browser"
msgstr "இணைய உலாவிக்கான சின்னத்தைத் தேர்வுசெய்ய விடும்"

#: qml/ConfigAppearance.qml:103
#, kde-format
msgctxt "@item:inmenu Reset icon to default"
msgid "Reset to default icon"
msgstr "இயல்பிருப்பு சின்னத்தை மீட்டமை"

#: qml/ConfigAppearance.qml:118
#, kde-format
msgctxt "@option:check"
msgid "Display Navigation Bar"
msgstr "உலாவல் பட்டையைக் காட்டு"

#: qml/ConfigGeneral.qml:22
#, kde-format
msgctxt "@option:radio"
msgid "Load last-visited page"
msgstr "கடைசியாக பார்த்த பக்கத்தை ஏற்று"

#: qml/ConfigGeneral.qml:26
#, kde-format
msgctxt "@title:group"
msgid "On startup:"
msgstr "துவக்கத்தில்"

#: qml/ConfigGeneral.qml:33
#, kde-format
msgctxt "@option:radio"
msgid "Always load this page:"
msgstr "எப்போதும் இப்பக்கத்தை ஏற்று:"

#: qml/ConfigGeneral.qml:65 qml/main.qml:119
#, kde-format
msgctxt "@info"
msgid "Type a URL"
msgstr "முகவரியை உள்ளிடவும்"

#: qml/ConfigGeneral.qml:76
#, kde-format
msgctxt "@title:group"
msgid "Content scaling:"
msgstr "உருப்பெருக்கம்:"

#: qml/ConfigGeneral.qml:80
#, kde-format
msgctxt "@option:radio"
msgid "Fixed scale:"
msgstr "மாறா உருப்பெருக்கம்:"

#: qml/ConfigGeneral.qml:115
#, kde-format
msgctxt "@option:radio"
msgid "Automatic scaling if width is below"
msgstr "இதைவிட அகலம் குறைவாக இருந்தால் உருப்பெருக்கு"

#: qml/ConfigGeneral.qml:151
#, kde-format
msgctxt "@title:group"
msgid "Browser Settings:"
msgstr "உலாவி அமைப்புகள்:"

#: qml/ConfigGeneral.qml:155
#, kde-format
msgctxt "@option:radio"
msgid "Private Browsing"
msgstr "கமுக்க உலாவல்"

#: qml/main.qml:74
#, kde-format
msgctxt "@action:button"
msgid "Go Back"
msgstr "முந்தைய பக்கத்திற்கு செல்"

#: qml/main.qml:84
#, kde-format
msgctxt "@action:button"
msgid "Go Forward"
msgstr "பிந்தைய பக்கத்திற்கு செல்"

#: qml/main.qml:95
#, kde-format
msgctxt "@action:button"
msgid "Go Home"
msgstr "முகப்புக்கு செல்"

#: qml/main.qml:100
#, kde-format
msgid "Open Default URL"
msgstr "இயல்பிருப்பு முகவரியைத் திற"

#: qml/main.qml:159
#, kde-format
msgctxt "@action:button"
msgid "Stop Loading This Page"
msgstr "இப்பக்கத்தை ஏற்றுவதை நிறுத்து"

#: qml/main.qml:159
#, kde-format
msgctxt "@action:button"
msgid "Reload This Page"
msgstr "இப்பக்கத்தை மீளேற்று"

#: qml/main.qml:173
#, kde-format
msgctxt ""
"@action:button keep this widget's pop-up open until explicitly dismissed"
msgid "Keep open"
msgstr "திறந்தே வைத்திரு"

#: qml/main.qml:227
#, kde-format
msgctxt "@action:inmenu"
msgid "Open Link in Browser"
msgstr "இணைப்பை உலாவியில் திற"

#: qml/main.qml:233
#, kde-format
msgctxt "@action:inmenu"
msgid "Copy Link Address"
msgstr "இணைப்பின் முகவரியை நகலெடு"

#: qml/main.qml:300
#, kde-format
msgctxt "An unwanted popup was blocked"
msgid "Popup blocked"
msgstr "மேல்தோன்றும் சாளரம் தடுக்கப்பட்டுள்ளது"

#: qml/main.qml:301
#, kde-format
msgid ""
"Click here to open the following blocked popup:\n"
"%1"
msgstr ""
"பின்வரும் தடுக்கப்பட்டுள்ள சாளரத்தைத் திறக்க கிளிக் செய்யவும்:\n"
"%1"
