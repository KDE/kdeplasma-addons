#: qml/config.qml:13
#, kde-format
msgctxt "@title"
msgid "Media"
msgstr "Multimèdia"

#: qml/config.qml:18
#, kde-format
msgctxt "@title"
msgid "Behavior"
msgstr "Comportament"

#: qml/ConfigBehavior.qml:48
#, kde-format
msgid "Change picture every:"
msgstr "Canvia la imatge cada:"

#: qml/ConfigBehavior.qml:65
#, kde-format
msgid "Hours"
msgstr "Hores"

#: qml/ConfigBehavior.qml:79
#, kde-format
msgid "Minutes"
msgstr "Minuts"

#: qml/ConfigBehavior.qml:93
#, kde-format
msgid "Seconds"
msgstr "Segons"

#: qml/ConfigBehavior.qml:103
#, kde-format
msgctxt "@label:listbox"
msgid "Image fill mode:"
msgstr "Mode d'emplenament de la imatge:"

#: qml/ConfigBehavior.qml:110
#, kde-format
msgctxt "@item:inlistbox"
msgid "Stretch"
msgstr "Estira"

#: qml/ConfigBehavior.qml:111
#, kde-format
msgctxt "@info"
msgid "The image is scaled to fit the frame"
msgstr "S'escala la imatge per a ajustar-la al marc"

#: qml/ConfigBehavior.qml:115
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect fit"
msgstr "Conserva l'aspecte d'ajustament"

#: qml/ConfigBehavior.qml:116
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fit without cropping"
msgstr "S'escala uniformement la imatge per a ajustar-la sense retallar"

#: qml/ConfigBehavior.qml:120
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect crop"
msgstr "Conserva l'aspecte de retall"

#: qml/ConfigBehavior.qml:121
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fill, cropping if necessary"
msgstr "S'escala uniformement la imatge per a omplir, retallant si cal"

#: qml/ConfigBehavior.qml:125
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile"
msgstr "Mosaic"

#: qml/ConfigBehavior.qml:126
#, kde-format
msgctxt "@info"
msgid "The image is duplicated horizontally and vertically"
msgstr "La imatge es duplica horitzontalment i verticalment"

#: qml/ConfigBehavior.qml:130
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile vertically"
msgstr "Mosaic vertical"

#: qml/ConfigBehavior.qml:131
#, kde-format
msgctxt "@info"
msgid "The image is stretched horizontally and tiled vertically"
msgstr "La imatge s'estira horitzontalment i amb mosaic vertical"

#: qml/ConfigBehavior.qml:135
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile horizontally"
msgstr "Mosaic horitzontal"

#: qml/ConfigBehavior.qml:136
#, kde-format
msgctxt "@info"
msgid "The image is stretched vertically and tiled horizontally"
msgstr "La imatge s'estira verticalment i amb mosaic horitzontal"

#: qml/ConfigBehavior.qml:140
#, kde-format
msgctxt "@item:inlistbox"
msgid "Pad"
msgstr "Normal"

#: qml/ConfigBehavior.qml:141
#, kde-format
msgctxt "@info"
msgid "The image is not transformed"
msgstr "La imatge no es transforma"

#: qml/ConfigBehavior.qml:186
#, kde-format
msgctxt "@label:checkbox"
msgid "General:"
msgstr "General:"

#: qml/ConfigBehavior.qml:187
#, kde-format
msgctxt "@option:check"
msgid "Randomize order"
msgstr "Ordre aleatori"

#: qml/ConfigBehavior.qml:192
#, kde-format
msgctxt "@option:check"
msgid "Pause slideshow when cursor is over image"
msgstr ""
"Fes pausa en la presentació de diapositives quan el cursor estiga damunt de "
"la imatge"

#: qml/ConfigBehavior.qml:197
#, kde-format
msgctxt "@option:check"
msgid "Click on image to open in external application"
msgstr "Cliqueu damunt de la imatge per a obrir-la en una aplicació externa"

#: qml/ConfigMedia.qml:27
#, kde-format
msgctxt "@action:button"
msgid "Add Folder…"
msgstr "Afig una carpeta…"

#: qml/ConfigMedia.qml:32
#, kde-format
msgctxt "@action:button"
msgid "Add Files…"
msgstr "Afig fitxers…"

#: qml/ConfigMedia.qml:84
#, kde-format
msgctxt "@action:button tooltip on icon-only button"
msgid "Remove path"
msgstr "Elimina el camí"

#: qml/ConfigMedia.qml:94
#, kde-format
msgctxt "@info placeholdermessage if no paths set"
msgid "No media configured"
msgstr "No s'ha configurat cap element multimèdia"

#: qml/ConfigMedia.qml:95
#, kde-format
msgctxt "@info placeholdermessage explanation"
msgid "Add files or folders to display them in the widget."
msgstr "Afig fitxers o carpetes per a mostrar-los al giny."

#: qml/ConfigMedia.qml:101
#, kde-format
msgctxt "@title:window"
msgid "Choose Files"
msgstr "Trieu els fitxers"

#: qml/ConfigMedia.qml:121
#, kde-format
msgctxt "@title:window"
msgid "Choose a Folder"
msgstr "Trieu una carpeta"

#: qml/main.qml:360
#, kde-format
msgctxt "@info placeholdermessage if no media loaded"
msgid "No media selected"
msgstr "No s'ha seleccionat cap element multimèdia"

#: qml/main.qml:364 qml/main.qml:374
#, kde-format
msgctxt "@action:button opens settings dialog"
msgid "Choose media…"
msgstr "Trieu un element multimèdia…"
