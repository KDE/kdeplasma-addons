#: config/qml/AlternateCalendarConfig.qml:48
#, kde-format
msgctxt "@label:listbox"
msgid "Calendar system:"
msgstr "历法："

#: config/qml/AlternateCalendarConfig.qml:69
#, kde-format
msgctxt "@info:tooltip"
msgid ""
"This calendar is based on pure astronomical calculation. It doesn't consider "
"any crescent visibility criteria."
msgstr "此日历基于单纯的天文学计算，它并不反映实际可观测的月相。"

#: config/qml/AlternateCalendarConfig.qml:77
#, kde-format
msgctxt "@label:spinbox"
msgid "Date offset:"
msgstr "日期偏移："

#: config/qml/AlternateCalendarConfig.qml:88
#, kde-format
msgid "%1 day"
msgid_plural "%1 days"
msgstr[0] "%1 天"

#: config/qml/AlternateCalendarConfig.qml:91
#, kde-format
msgctxt "@info:tooltip"
msgid ""
"A positive offset signifies a later date, while a negative offset signifies "
"an earlier date."
msgstr "正偏移表示较晚日期，负偏移表示较早日期。"

#: provider/hebrewcalendar.cpp:91
#, kde-format
msgctxt ""
"%1 Day number %2 Translated month name in Hebrew/Jewish calendar %3 Year "
"number %4 Full date in Hebrew"
msgid "%1 %2, %3 (%4)"
msgstr "%3 年 %2 %1 日 (%4)"

#: provider/indiancalendar.cpp:70
#, kde-format
msgctxt "@label %1 day %2 month name in India National Calendar %3 year"
