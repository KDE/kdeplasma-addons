#: package/contents/config/config.qml:13
#, kde-format
msgctxt "@title"
msgid "Media"
msgstr "媒体"

#: package/contents/config/config.qml:18
#, kde-format
msgctxt "@title"
msgid "Behavior"
msgstr "行为"

#: package/contents/ui/ConfigBehavior.qml:48
#, kde-format
msgid "Change picture every:"
msgstr "图像更换间隔时间："

#: package/contents/ui/ConfigBehavior.qml:65
#, kde-format
msgid "Hours"
msgstr "小时"

#: package/contents/ui/ConfigBehavior.qml:79
#, kde-format
msgid "Minutes"
msgstr "分"

#: package/contents/ui/ConfigBehavior.qml:93
#, kde-format
msgid "Seconds"
msgstr "秒"

#: package/contents/ui/ConfigBehavior.qml:103
#, kde-format
msgctxt "@label:listbox"
msgid "Image fill mode:"
msgstr "图像填充模式："

#: package/contents/ui/ConfigBehavior.qml:110
#, kde-format
msgctxt "@item:inlistbox"
msgid "Stretch"
msgstr "拉伸"

#: package/contents/ui/ConfigBehavior.qml:111
#, kde-format
msgctxt "@info"
msgid "The image is scaled to fit the frame"
msgstr "图片将缩放到适合相框的大小"

#: package/contents/ui/ConfigBehavior.qml:115
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect fit"
msgstr "保持宽高比缩放"

#: package/contents/ui/ConfigBehavior.qml:116
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fit without cropping"
msgstr "图像将保持原有宽高比缩放到适合大小，不进行裁剪"

#: package/contents/ui/ConfigBehavior.qml:120
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect crop"
msgstr "保持宽高比缩放并裁剪"

#: package/contents/ui/ConfigBehavior.qml:121
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fill, cropping if necessary"
msgstr "图像将保持原有宽高比缩放到适合大小，按需进行裁剪"

#: package/contents/ui/ConfigBehavior.qml:125
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile"
msgstr "平铺"

#: package/contents/ui/ConfigBehavior.qml:126
#, kde-format
msgctxt "@info"
msgid "The image is duplicated horizontally and vertically"
msgstr "图片将在水平和垂直方向上重复显示"

#: package/contents/ui/ConfigBehavior.qml:130
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile vertically"
msgstr "垂直平铺"

#: package/contents/ui/ConfigBehavior.qml:131
#, kde-format
msgctxt "@info"
msgid "The image is stretched horizontally and tiled vertically"
msgstr "图片将被水平拉伸，然后垂直平铺"

#: package/contents/ui/ConfigBehavior.qml:135
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile horizontally"
msgstr "水平平铺"

#: package/contents/ui/ConfigBehavior.qml:136
#, kde-format
msgctxt "@info"
msgid "The image is stretched vertically and tiled horizontally"
msgstr "图片将被垂直拉伸，然后水平平铺"

#: package/contents/ui/ConfigBehavior.qml:140
#, kde-format
msgctxt "@item:inlistbox"
msgid "Pad"
msgstr "原图"

#: package/contents/ui/ConfigBehavior.qml:141
#, kde-format
msgctxt "@info"
msgid "The image is not transformed"
msgstr "图片将不进行变形处理"

#: package/contents/ui/ConfigBehavior.qml:186
#, kde-format
msgctxt "@label:checkbox"
msgid "General:"
msgstr "常规："

#: package/contents/ui/ConfigBehavior.qml:187
#, kde-format
msgctxt "@option:check"
msgid "Randomize order"
msgstr "随机顺序"

#: package/contents/ui/ConfigBehavior.qml:192
#, kde-format
msgctxt "@option:check"
msgid "Pause slideshow when cursor is over image"
msgstr "鼠标光标悬停在图像上时暂停幻灯片切换"

#: package/contents/ui/ConfigBehavior.qml:197
#, kde-format
msgctxt "@option:check"
msgid "Click on image to open in external application"
msgstr "点击图像在外部程序中打开"

#: package/contents/ui/ConfigMedia.qml:27
#, kde-format
msgctxt "@action:button"
msgid "Add Folder…"
msgstr "添加文件夹…"

#: package/contents/ui/ConfigMedia.qml:32
#, kde-format
msgctxt "@action:button"
msgid "Add Files…"
msgstr "添加文件…"

#: package/contents/ui/ConfigMedia.qml:82
#, kde-format
msgctxt "@action:button tooltip on icon-only button"
msgid "Remove path"
msgstr "移除路径"

#: package/contents/ui/ConfigMedia.qml:92
#, kde-format
msgctxt "@info placeholdermessage if no paths set"
msgid "No media configured"
msgstr "没有配置媒体"

#: package/contents/ui/ConfigMedia.qml:93
#, kde-format
msgctxt "@info placeholdermessage explanation"
msgid "Add files or folders to display them in the widget."
msgstr "添加文件或文件夹以在挂件中显示它们。"

#: package/contents/ui/ConfigMedia.qml:99
#, kde-format
msgctxt "@title:window"
msgid "Choose Files"
msgstr "选择文件"

#: package/contents/ui/ConfigMedia.qml:119
#, kde-format
msgctxt "@title:window"
msgid "Choose a Folder"
msgstr "选择一个文件夹"

#: package/contents/ui/main.qml:361
#, kde-format
msgctxt "@info placeholdermessage if no media loaded"
msgid "No media selected"
msgstr "没有选择媒体"

#: package/contents/ui/main.qml:365 package/contents/ui/main.qml:375
#, kde-format
msgctxt "@action:button opens settings dialog"
msgid "Choose media…"
msgstr "选择媒体…"
