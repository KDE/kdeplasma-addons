msgctxt "@title"
msgid "Media"
msgstr "Medio"

#: qml/config.qml:18
#, kde-format
msgctxt "@title"
msgid "Behavior"
msgstr "Comportamiento"

#: qml/ConfigBehavior.qml:48
#, kde-format
msgid "Change picture every:"
msgstr "Cambiar la imagen cada:"

#: qml/ConfigBehavior.qml:65
#, kde-format
msgid "Hours"
msgstr "horas"

#: qml/ConfigBehavior.qml:79
#, kde-format
msgid "Minutes"
msgstr "minutos"

#: qml/ConfigBehavior.qml:93
#, kde-format
msgid "Seconds"
msgstr "segundos"

#: qml/ConfigBehavior.qml:103
#, kde-format
msgctxt "@label:listbox"
msgid "Image fill mode:"
msgstr "Modo de relleno de la imagen:"

#: qml/ConfigBehavior.qml:110
#, kde-format
msgctxt "@item:inlistbox"
msgid "Stretch"
msgstr "Estirar"

#: qml/ConfigBehavior.qml:111
#, kde-format
msgctxt "@info"
msgid "The image is scaled to fit the frame"
msgstr "La imagen se escala para que se ajuste al marco"

#: qml/ConfigBehavior.qml:115
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect fit"
msgstr "Preservar el aspecto ajustando"

#: qml/ConfigBehavior.qml:116
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fit without cropping"
msgstr "La imagen se escala uniformemente para que se ajuste sin recortar"

#: qml/ConfigBehavior.qml:120
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect crop"
msgstr "Preservar el aspecto recortando"

#: qml/ConfigBehavior.qml:121
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fill, cropping if necessary"
msgstr ""
"La imagen se escala uniformemente para que se ajuste, recortando si es "
"necesario"

#: qml/ConfigBehavior.qml:125
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile"
msgstr "Mosaico"

#: qml/ConfigBehavior.qml:126
#, kde-format
msgctxt "@info"
msgid "The image is duplicated horizontally and vertically"
msgstr "La imagen se duplica horizontal y verticalmente"

#: qml/ConfigBehavior.qml:130
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile vertically"
msgstr "Mosaico vertical"

#: qml/ConfigBehavior.qml:131
#, kde-format
msgctxt "@info"
msgid "The image is stretched horizontally and tiled vertically"
msgstr "La imagen se estira horizontalmente y se pone en mosaico verticalmente"

#: qml/ConfigBehavior.qml:135
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile horizontally"
msgstr "Mosaico horizontal"

#: qml/ConfigBehavior.qml:136
#, kde-format
msgctxt "@info"
msgid "The image is stretched vertically and tiled horizontally"
msgstr "La imagen se estira verticalmente y se pone en mosaico horizontalmente"

#: qml/ConfigBehavior.qml:140
#, kde-format
msgctxt "@item:inlistbox"
msgid "Pad"
msgstr "Rellenar con espacio"

#: qml/ConfigBehavior.qml:141
#, kde-format
msgctxt "@info"
msgid "The image is not transformed"
msgstr "La imagen no se transforma"

#: qml/ConfigBehavior.qml:186
#, kde-format
msgctxt "@label:checkbox"
msgid "General:"
msgstr "General:"

#: qml/ConfigBehavior.qml:187
#, kde-format
msgctxt "@option:check"
msgid "Randomize order"
msgstr "Orden aleatorio"

#: qml/ConfigBehavior.qml:192
#, kde-format
msgctxt "@option:check"
msgid "Pause slideshow when cursor is over image"
msgstr "Pausar la presentación al situar el cursor sobre la imagen"

#: qml/ConfigBehavior.qml:197
#, kde-format
msgctxt "@option:check"
msgid "Click on image to open in external application"
msgstr "Clic sobre la imagen para abrirla en una aplicación externa"

#: qml/ConfigMedia.qml:27
#, kde-format
msgctxt "@action:button"
msgid "Add Folder…"
msgstr "Añadir carpeta…"

#: qml/ConfigMedia.qml:32
#, kde-format
msgctxt "@action:button"
msgid "Add Files…"
msgstr "Añadir archivos…"

#: qml/ConfigMedia.qml:84
#, kde-format
