#: datetimerunner.cpp:17
#, kde-format
msgctxt "Note this is a KRunner keyword"
msgid "date"
msgstr "fecha"

#: datetimerunner.cpp:18
#, kde-format
msgctxt "Note this is a KRunner keyword"
msgid "time"
msgstr "hora"

#: datetimerunner.cpp:25
#, kde-format
msgid "Displays the current date"
msgstr "Muestra la fecha actual"

#: datetimerunner.cpp:26
#, kde-format
msgid "Displays the current time"
msgstr "Muestra la hora actual"

#: datetimerunner.cpp:27 datetimerunner.cpp:29
#, kde-format
msgctxt "The <> and space are part of the example query"
msgid " <timezone>"
msgstr " <zonahoraria>"

#: datetimerunner.cpp:28
#, kde-format
msgid "Displays the current date in a given timezone"
msgstr "Muestra la fecha actual en la zona horaria indicada"

#: datetimerunner.cpp:30
#, kde-format
msgid "Displays the current time in a given timezone"
msgstr "Muestra la hora actual en la zona horaria indicada"

#: datetimerunner.cpp:43
#, kde-format
msgid "Today's date is %1"
msgstr "La fecha de hoy es %1"

#: datetimerunner.cpp:57
#, kde-format
msgid "Current time is %1"
msgstr "La hora actual es %1"

#~ msgid "The date in %1 is %2"
#~ msgstr "La fecha en %1 es %2"

#~ msgid "The current time in %1 is %2"
#~ msgstr "La hora actual en %1 es %2"
