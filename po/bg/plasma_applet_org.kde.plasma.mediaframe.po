#: qml/config.qml:13
#, kde-format
msgctxt "@title"
msgid "Media"
msgstr "Медия"

#: qml/config.qml:18
#, kde-format
msgctxt "@title"
msgid "Behavior"
msgstr "Поведение"

#: qml/ConfigBehavior.qml:48
#, kde-format
msgid "Change picture every:"
msgstr "Сменяне на снимката всеки:"

#: qml/ConfigBehavior.qml:65
#, kde-format
msgid "Hours"
msgstr "Часове"

#: qml/ConfigBehavior.qml:79
#, kde-format
msgid "Minutes"
msgstr "Минути"

#: qml/ConfigBehavior.qml:93
#, kde-format
msgid "Seconds"
msgstr "Секунди"

#: qml/ConfigBehavior.qml:103
#, kde-format
msgctxt "@label:listbox"
msgid "Image fill mode:"
msgstr "Режим на запълване на изображение:"

#: qml/ConfigBehavior.qml:110
#, kde-format
msgctxt "@item:inlistbox"
msgid "Stretch"
msgstr "Разтегнато"

#: qml/ConfigBehavior.qml:111
#, kde-format
msgctxt "@info"
msgid "The image is scaled to fit the frame"
msgstr "Изображението се мащабира, за да се побере в рамката"

#: qml/ConfigBehavior.qml:115
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect fit"
msgstr "Запазване на пропорциите"

#: qml/ConfigBehavior.qml:116
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fit without cropping"
msgstr ""
"Изображението се мащабира равномерно, за да се побере, без да се изрязва"

#: qml/ConfigBehavior.qml:120
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect crop"
msgstr "Запазване на пропорциите с изрязване"

#: qml/ConfigBehavior.qml:121
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fill, cropping if necessary"
msgstr ""
"Изображението се мащабира равномерно, за да се побере, като се изрязва, ако "
"е необходимо"

#: qml/ConfigBehavior.qml:125
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile"
msgstr "Плочки"

#: qml/ConfigBehavior.qml:126
#, kde-format
msgctxt "@info"
msgid "The image is duplicated horizontally and vertically"
msgstr "Изображението се повтаря хоризонтално и вертикално"

#: qml/ConfigBehavior.qml:130
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile vertically"
msgstr "Плочки вертикално"

#: qml/ConfigBehavior.qml:131
#, kde-format
msgctxt "@info"
msgid "The image is stretched horizontally and tiled vertically"
msgstr ""
"Изображението се разгъва хоризонтално и се разпределя като плочки вертикално"

#: qml/ConfigBehavior.qml:135
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile horizontally"
msgstr "Плочки хоризонтално"

#: qml/ConfigBehavior.qml:136
#, kde-format
msgctxt "@info"
msgid "The image is stretched vertically and tiled horizontally"
msgstr ""
"Изображението се разгъва вертикално и се разпределя като плочки хоризонтално"

#: qml/ConfigBehavior.qml:140
#, kde-format
msgctxt "@item:inlistbox"
msgid "Pad"
msgstr "Подложка"

#: qml/ConfigBehavior.qml:141
#, kde-format
msgctxt "@info"
msgid "The image is not transformed"
msgstr "Изображението не се трансформира"

#: qml/ConfigBehavior.qml:186
#, kde-format
msgctxt "@label:checkbox"
msgid "General:"
msgstr "Общи:"

#: qml/ConfigBehavior.qml:187
#, kde-format
msgctxt "@option:check"
msgid "Randomize order"
msgstr "Случаен ред"

#: qml/ConfigBehavior.qml:192
#, kde-format
msgctxt "@option:check"
msgid "Pause slideshow when cursor is over image"
msgstr "Пауза на слайдшоуто, когато курсорът е над изображението"

#: qml/ConfigBehavior.qml:197
#, kde-format
msgctxt "@option:check"
msgid "Click on image to open in external application"
msgstr ""
"Отваряне на изображението във външно приложение при щракване върху "
"изображение"

#: qml/ConfigMedia.qml:27
#, kde-format
msgctxt "@action:button"
msgid "Add Folder…"
msgstr "Добавяне на папка…"

#: qml/ConfigMedia.qml:32
#, kde-format
msgctxt "@action:button"
msgid "Add Files…"
msgstr "Добавяне на файлове…"

#: qml/ConfigMedia.qml:84
#, kde-format
msgctxt "@action:button tooltip on icon-only button"
msgid "Remove path"
msgstr "Премахване на пътя"

#: qml/ConfigMedia.qml:94
#, kde-format
msgctxt "@info placeholdermessage if no paths set"
msgid "No media configured"
msgstr "Няма конфигурирана медия"

#: qml/ConfigMedia.qml:95
#, kde-format
msgctxt "@info placeholdermessage explanation"
msgid "Add files or folders to display them in the widget."
msgstr "Добавете файлове или папки, за да ги покажете в уиджета."

#: qml/ConfigMedia.qml:101
#, kde-format
msgctxt "@title:window"
msgid "Choose Files"
msgstr "Избиране на файлове"

#: qml/ConfigMedia.qml:121
#, kde-format
msgctxt "@title:window"
msgid "Choose a Folder"
msgstr "Избиране на папка"

#: qml/main.qml:360
#, kde-format
msgctxt "@info placeholdermessage if no media loaded"
msgid "No media selected"
msgstr "Няма избрана медия"

#: qml/main.qml:364 qml/main.qml:374
#, kde-format
msgctxt "@action:button opens settings dialog"
msgid "Choose media…"
msgstr "Избиране на медия…"
