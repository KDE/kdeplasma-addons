#: colorsrunner.cpp:29
#, fuzzy, kde-format
#| msgid ""
#| "Diplays the value of :q: as a color when :q: is a hexadecimal color (e.g. "
#| "#FF00FF) or an SVG named color."
msgid ""
"Displays the value of :q: as a color when :q: is a hexadecimal color (e.g. "
"#FF00FF) or an SVG named color."
msgstr ""
":q: bir onaltılık renk (örneğin, #FF00FF) veya bir SVG adlı renk iken :q: "
"yer tutucusunu bir renk değeri olarak görüntüler."

#: colorsrunner.cpp:31
#, kde-format
msgid ""
"Displays the value of :q: as a color when :q: is an RGB color triplet (e.g. "
"255,255,255)."
msgstr ""
":q: bir RGB rengi (örneğin, 255, 255, 255) iken :q: yer tutucusunu bir renk "
"değeri olarak görüntüler."

#: colorsrunner.cpp:34
#, kde-format
msgid ""
"Displays the value of :q: as a color with alpha channel when :q: is an RGBA "
"color quadruplet (e.g. 255,255,255,255)."
msgstr ""
":q: bir RGB rengi dörtlüsü (örneğin, 255, 255, 255, 255) iken :q: yer "
"tutucusunu alfa kanallı bir renk değeri olarak görüntüler."

#: colorsrunner.cpp:139
#, kde-format
msgid "RGBA: %1, %2, %3, %4"
msgstr "RGBA: %1, %2, %3, %4"

#: colorsrunner.cpp:141
#, kde-format
msgid "RGB: %1, %2, %3"
msgstr "RGB: %1, %2, %3"

#: colorsrunner.cpp:146
#, kde-format
msgid "CMYK: %1%, %2%, %3%, %4%"
msgstr "CMYK: %​%1, %​%2, %​%3, %​%4"
