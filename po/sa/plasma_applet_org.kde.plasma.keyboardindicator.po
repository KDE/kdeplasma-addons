#: config.qml:13
#, kde-format
msgctxt "@title"
msgid "Keys"
msgstr "कीलम्"

#: configAppearance.qml:37
#, kde-format
msgctxt ""
"@label show keyboard indicator when Caps Lock or Num Lock is activated"
msgid "Show when activated:"
msgstr "सक्रियतायां दर्शयतु:"

#: configAppearance.qml:40
#, kde-format
msgctxt "@option:check"
msgid "Caps Lock"
msgstr "टोपी ताला"

#: configAppearance.qml:47
#, kde-format
msgctxt "@option:check"
msgid "Num Lock"
msgstr "नुम् ताला"

#: main.qml:114
#, kde-format
msgid "Alt is latched"
msgstr "अल्ट् लट् भवति"

#: main.qml:116
#, kde-format
msgid "Alt is locked"
msgstr "अल्ट् तालाबद्धः अस्ति"

#: main.qml:120
#, kde-format
msgid "Shift is latched"
msgstr "शिफ्ट् लट् भवति"

#: main.qml:122
#, kde-format
msgid "Shift is locked"
msgstr "शिफ्टं ताडितम् अस्ति"

#: main.qml:126
#, kde-format
msgid "Control is latched"
msgstr "नियन्त्रणं लट् भवति"

#: main.qml:128
#, kde-format
msgid "Control is locked"
msgstr "नियन्त्रणं ताडितम् अस्ति"

#: main.qml:132
#, kde-format
msgid "Meta is latched"
msgstr "मेटा लट् भवति"

#: main.qml:134
#, kde-format
msgid "Meta is locked"
msgstr "मेटा तालाबद्धः अस्ति"

#: main.qml:138
#, kde-format
msgid "Alt Gr is latched"
msgstr "Alt Gr इति लट् भवति"

#: main.qml:140
#, kde-format
msgid "Alt Gr is locked"
msgstr "Alt Gr तालाबद्धः अस्ति"

#: main.qml:144
#, kde-format
msgid "Caps Lock activated"
msgstr "Caps Lock सक्रियम्"

#: main.qml:147
#, kde-format
msgid "Num Lock activated"
msgstr "Num Lock सक्रियम्"

#: main.qml:155
#, kde-format
msgid "No lock keys activated"
msgstr "न तालाकुंजी सक्रियताम्"
