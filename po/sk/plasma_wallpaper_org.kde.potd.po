#: package/contents/ui/config.qml:44
#, kde-format
msgctxt "@label:listbox"
msgid "Provider:"
msgstr "Poskytovateľ:"

#: package/contents/ui/config.qml:63
#, kde-format
msgctxt "@info:tooltip"
msgid ""
"This wallpaper provider does not filter out images that may be sensitive or "
"objectionable. Use caution if these images will be displayed in public."
msgstr ""
"Tento poskytovateľ tapiet nefiltruje obrázky, ktoré môžu byť citlivé alebo "
"nevhodné. Ak budú tieto obrázky vystavené na verejnosti, buďte opatrní."

#: package/contents/ui/config.qml:72
#, kde-format
msgctxt "@option:check"
msgid "Update when using metered network connection"
msgstr "Aktualizácia pri používaní meraného sieťového pripojenia"

#: package/contents/ui/config.qml:81
#, kde-format
msgctxt "@label:listbox"
msgid "Positioning:"
msgstr "Umiestnenie:"

#: package/contents/ui/config.qml:84
#, kde-format
msgctxt "@item:inlistbox"
msgid "Scaled and Cropped"
msgstr "Zväčšený a orezaný"

#: package/contents/ui/config.qml:88
