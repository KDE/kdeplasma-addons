#: colorsrunner.cpp:29
#, fuzzy, kde-format
#| msgid ""
#| "Diplays the value of :q: as a color when :q: is a hexadecimal color (e.g. "
#| "#FF00FF) or an SVG named color."
msgid ""
"Displays the value of :q: as a color when :q: is a hexadecimal color (e.g. "
"#FF00FF) or an SVG named color."
msgstr ""
"Visar värdet på :q: som en färg när :q: är en hexadecimal färg (t.ex. "
"#FF00FF) eller en SVG namngiven färg."

#: colorsrunner.cpp:31
#, kde-format
msgid ""
"Displays the value of :q: as a color when :q: is an RGB color triplet (e.g. "
"255,255,255)."
msgstr ""
"Visar värdet på :q: som en färg när :q: är  en RGB-färgtrippel (t.ex. "
"255,255,255)."

#: colorsrunner.cpp:34
#, kde-format
msgid ""
"Displays the value of :q: as a color with alpha channel when :q: is an RGBA "
"color quadruplet (e.g. 255,255,255,255)."
msgstr ""
"Visar värdet på :q: som en färg med alfakanal när :q: är en RGBA-"
"färgkvadrupel (t.ex. 255,255,255,255)."

#: colorsrunner.cpp:139
#, kde-format
msgid "RGBA: %1, %2, %3, %4"
msgstr "RGBA: %1, %2, %3, %4"

#: colorsrunner.cpp:141
#, kde-format
msgid "RGB: %1, %2, %3"
msgstr "RGB: %1, %2, %3"

#: colorsrunner.cpp:146
#, kde-format
msgid "CMYK: %1%, %2%, %3%, %4%"
msgstr "CMYK: %1%, %2%, %3%, %4%"
