msgid "Stretch"
msgstr "Allunga"

#: package/contents/ui/ConfigBehavior.qml:111
#, kde-format
msgctxt "@info"
msgid "The image is scaled to fit the frame"
msgstr "L'immagine è ridimensionata per adattarsi alla cornice"

#: package/contents/ui/ConfigBehavior.qml:115
#, kde-format
msgctxt "@item:inlistbox"
msgid "Preserve aspect fit"
msgstr "Preserva le proporzioni"

#: package/contents/ui/ConfigBehavior.qml:116
#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fit without cropping"
msgstr "L'immagine è ridimensionata in modo uniforme senza ritagli"

#: package/contents/ui/ConfigBehavior.qml:120
#, kde-format
msgctxt "@item:inlistbox"
