msgid "The image is stretched horizontally and tiled vertically"
msgstr "L'immagine è allungata orizzontalmente e affiancata verticalmente"

#: package/contents/ui/ConfigBehavior.qml:135
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile horizontally"
msgstr "Affianca orizzontalmente"

#: package/contents/ui/ConfigBehavior.qml:136
#, kde-format
