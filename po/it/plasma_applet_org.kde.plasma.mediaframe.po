#, kde-format
msgctxt "@info"
msgid "The image is scaled uniformly to fill, cropping if necessary"
msgstr ""
"L'immagine è ridimensionata in modo uniforme per adattarsi, con ritaglio se "
"necessario"

#: package/contents/ui/ConfigBehavior.qml:125
#, kde-format
msgctxt "@item:inlistbox"
msgid "Tile"
msgstr "Affianca"

